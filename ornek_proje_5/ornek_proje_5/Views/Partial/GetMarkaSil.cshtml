@using EntityFrameworkLibrary
@using ornek_proje_5.Controllers
@model PageItem

@{
    Layout = null;
}

<div id="mdlForm" class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="danger bigger">Silme işlemi</h4>
    </div>

    <div class="modal-body">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                @Model.lstMarkalarIslem.FirstOrDefault().markaBaslik isimli markayı silmek istediğinize emin misiniz?
            </div>
        </div>
    </div>

    <div class="modal-footer">
        <button class="btn btn-sm" data-dismiss="modal">
            <i class="ace-icon fa fa-times"></i>
            Vazgeç
        </button>
        <a class="btn btn-danger" data-mode="MarkaSilTamamla" data-data1="@Model.lstMarkalarIslem.FirstOrDefault().markaID">Sil</a>
    </div>
</div>


